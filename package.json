{
  "name": "@th0masb/eth-client",
  "version": "0.2.2",
  "author": "Thomas Ball",
  "bin": {
    "eth-client": "./bin/run"
  },
  "bugs": "https://github.com/th0masb/eth-client/issues",
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.3.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "await-semaphore": "^0.1.3",
    "dotenv": "^10.0.0",
    "ethers": "^5.3.1",
    "ts-is-present": "^1.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^10.0.2",
    "prettier": "2.3.1",
    "semver": "^7.3.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/th0masb/eth-client",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "eth-client",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "th0masb/eth-client",
  "scripts": {
    "check-version": "semver --range \">$(npm view $npm_package_name version)\" $npm_package_version",
    "postpack": "rm -f oclif.manifest.json",
    "format": "prettier --write .",
    "build": "tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lintfix": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "readme": "oclif-dev readme",
    "standardise": "oclif-dev readme && prettier --write . && npm i"
  },
  "types": "lib/index.d.ts"
}
